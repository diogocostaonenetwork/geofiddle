    "wicket": "^1.3.3"
  },
  "devDependencies": {
    "coveralls": "^3.0.2",
    "css-loader": "^1.0.0",
    "cssnano": "^4.1.7",
    "html-loader": "^0.5.5",
    "html-webpack-plugin": "^3.2.0",
    "jest": "^23.6.0",
    "node-sass": "^4.9.4",
    "postcss-loader": "^3.0.0",
    "postcss-preset-env": "^6.2.0",
    "sass-loader": "^7.1.0",
    "style-loader": "^0.23.1",
