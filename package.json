    "webpack-cli": "^3.1.2",
    "webpack-dev-server": "^3.1.9",
    "webpack-merge": "^4.1.4"
  },
  "jest": {
    "collectCoverage": true,
    "coveragePathIgnorePatterns": [
      "<rootDir>/www/js/lib/"
    ],
    "moduleNameMapper": {
      "^underscore$": "lodash"
    },
    "modulePaths": [
      "<rootDir>/www/js/",
      "<rootDir>/node_modules"
    ],
    "testEnvironment": "node"
  },
  "scripts": {
    "test": "jest",
    "build": "webpack --config webpack.prd.js",
    "start": "webpack-dev-server --progress --config webpack.dev.js",
    "clean": "rm -rf ./node_modules ./coverage ./npm-debug.log .nvm/.cache .nvm/alias .nvm/versions ./dist"
  }
}
