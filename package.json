{
  "name": "GeoFiddle",
  "version": "0.9.0",
  "author": "Rui Pinge",
  "repository": {
    "type": "git",
    "url": "git@github.com:ruipinge/geofiddle.git"
  },
  "license": "MIT",
  "dependencies": {
    "jquery": "^3.3.1",
    "lodash": "^4.17.10",
    "backbone": "^1.3.3",
    "google-maps": "^3.3.0",
    "geodesy": "^1.1.3",
    "wicket": "^1.3.3"
  },
  "devDependencies": {
    "coveralls": "^3.0.2",
    "html-loader": "^0.5.5",
    "http-server": "^0.11.1",
    "jest": "^23.6.0",
    "webpack": "^4.21.0",
    "webpack-cli": "^3.1.2",
    "webpack-dev-server": "^3.1.9"
  },
  "jest": {
    "collectCoverage": true,
    "coveragePathIgnorePatterns": [
      "<rootDir>/www/js/lib/"
    ],
    "moduleNameMapper": {
      "^underscore$": "lodash"
    },
    "modulePaths": [
      "<rootDir>/www/js/",
      "<rootDir>/node_modules"
    ],
    "testEnvironment": "node"
  },
  "scripts": {
    "test": "jest",
    "build": "webpack --mode=production",
    "start": "webpack-dev-server --progress",
    "clean": "rm -rf ./node_modules ./coverage ./npm-debug.log .nvm/.cache .nvm/alias .nvm/versions ./dist"
  }
}
